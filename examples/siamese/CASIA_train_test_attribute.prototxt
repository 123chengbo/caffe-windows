  bottom: "pool5"
  top: "pool5"
  dropout_param {
    dropout_ratio: 0.6
  }
}
layer {
  name: "slice_pool5"
  type: "Slice"
  bottom: "pool5"
  slice_param {
    slice_point: 40
	axis: 0
  }
  top: "identity_feature"
  top: "attribute_feature"
}
layer {
  name: "identity_ip"
  type: "InnerProduct"
  bottom: "identity_feature"
  top: "identity_ip"
  param {
    lr_mult: 1
	decay_mult: 1e-5
	name: "identity_ip_w"
  }
  param {
    lr_mult: 1
	decay_mult: 0
	name: "identity_ip_b"
  }
  inner_product_param {
    num_output: 10575
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "accuracy_loss"
  type: "Accuracy"
  bottom: "ip2"
  bottom: "label1"
  top: "accuracy_loss"
  include: { phase: TEST }
}
layer {
  name: "softmax_loss1"
  type: "SoftmaxWithLoss"
#  hinge_loss_param {
#    norm: L2
#  }
  loss_weight: 1
  bottom: "ip2"
  bottom: "label1"
  top: "softmax_loss1"
}
layer {
  name: "attribute_ip"
  type: "InnerProduct"
  bottom: "attribute_feature"
  top: "attribute_ip"
  param {
    lr_mult: 1
	decay_mult: 1e-5
	name: "attribute_ip_w"
  }
  param {
    lr_mult: 2
	decay_mult: 0
	name: "attribute_ip_b"
  }
  inner_product_param {
    num_output: 80
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "reshape_attribute"
  type: "Reshape"
  bottom: "attribute_ip"
  top: "attribute_ip"
  reshape_param {
    shape {
      dim: 0  # copy the dimension from below
      dim: 2
      dim: 40
      dim: -1 # infer it from the other dimensions
    }
  }
}
layer {
  name: "attribute_loss"
  type: "SoftmaxWithLoss"
  loss_weight: 1
  bottom: "attribute_ip"
  bottom: "atrribute_label"
  top: "attribute_loss"
  softmax_param {
    axis: 1
  }
  
#  include: { phase: TRAIN }
}