name: "CASIA"
layer {
  name: "pair_data1"
  type: "ImageData"
  top: "pair_data1"
  top: "label1"
  image_data_param {
    source: "G:/100/list.txt"
    is_color: false
	batch_size: 40
  }
  transform_param { 
    mean_file: "examples/siamese/96.8/mean.proto"
	scale: 0.0078125
  }
  include: { phase: TRAIN }
}
layer {
  name: "attribute_data"
  type: "MultiLabelImageData"
  top: "attribute_data"
  top: "atrribute_label"
  image_data_param {
    source: "G:/celebrity/list_attr_celeba_cleaned.txt"
    is_color: false
    batch_size: 40
  }
  transform_param { 
    mean_file: "examples/siamese/96.8/mean.proto"
	scale: 0.0078125
  }
  include: { phase: TRAIN }
}
layer {
  name: "pair_data1"
  type: "ImageData"
  top: "pair_data1"
  top: "label1"
  image_data_param {
    source: "G:/100/list.txt"
    is_color: false
	batch_size: 40
  }
  transform_param { 
    mean_file: "examples/siamese/96.8/mean.proto"
	scale: 0.0078125
  }
  include: { phase: TEST }
}
layer {
  name: "attribute_data"
  type: "MultiLabelImageData"
  top: "attribute_data"
  top: "atrribute_label"
  image_data_param {
    source: "G:/celebrity/list_attr_celeba_cleaned.txt"
    is_color: false
    batch_size: 40
  }
  transform_param { 
    mean_file: "D:/ThirdPartyLibrary/caffe/examples/siamese/96.8/mean.proto"
	scale: 0.0078125
  }
  include: { phase: TEST }
}
layer {
  name: "concat_data"
  type: "Concat"
  bottom: "pair_data1"
  bottom: "attribute_data"
  top: "concat_data"
  concat_param {
    concat_dim: 0
  }
}
layer {
  name: "conv11"
  type: "Convolution"
  bottom: "concat_data"
  top: "conv11"
  param {
    lr_mult: 1
	decay_mult: 0
	name: "conv11_w"
  }
  param {
    lr_mult: 1
	decay_mult: 0
	name: "conv11_b"
  }
  convolution_param {
    num_output: 32
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "msra"
	  alpha: 0.25
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "relu11"
  type: "Insanity"
  bottom: "conv11"
  top: "conv11"
  insanity_param{
    lb: 3
	ub: 5
  }
}
layer {
  name: "conv12"
  type: "Convolution"
  bottom: "conv11"
  top: "conv12"
  param {
    lr_mult: 1
	decay_mult: 0
	name: "conv12_w"
  }
  param {
    lr_mult: 1
	decay_mult: 0
	name: "conv12_b"
  }
  convolution_param {
    num_output: 64
    kernel_size: 3
    stride: 1
	pad: 1
    weight_filler {
      type: "msra"
	  alpha: 0.25
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "relu12"
  type: "Insanity"
  bottom: "conv12"
  top: "conv12"
  insanity_param{
    lb: 3
	ub: 5
  }
}
layer {
  name: "pool1"
  type: "Pooling"
  bottom: "conv12"
  top: "pool1"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}
layer {
  name: "conv21"
  type: "Convolution"
  bottom: "pool1"
  top: "conv21"
